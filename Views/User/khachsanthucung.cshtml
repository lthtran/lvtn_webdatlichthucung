                        <p class="dotText wk-editable-text" id="sectionLP_11-text7">Cung cấp hình ảnh và video của các bé mỗi ngày</p>

                        <div class="fagopetSP_9">
                            <div class="selectBlock">
                                <input type="text" hidden name="serviceid" id="serviceid" />
                                @foreach (var listdv in Model.listDV)
                                {
                                    <p class="text_1 wk-editable-text" id="sectionLP_11-text14">@listdv.TenDV</p>
                                    <div class="wrapFlex">
                                        @foreach (var listct in Model.listCTDV)
                                        {
                                            if (listct.MaDV == listdv.MaDV)
                                            {
                                                <p class="typePet wk-editable-text" id="sectionLP_11-text15" serviceid="@listct.MaCT" priceroom="@listct.DonGia" priceformat="@String.Format("{0:0,0}", @listct.DonGia)Đ" typeroom="@listct.DichVu.TenDV">@listct.DieuKien</p>
                                            }
                                        }
                                    </div>
                                }
                            </div>
                            <script>
                                articles = document.getElementsByClassName('typePet');
                                var serviceid = document.getElementById("serviceid");

                                for (var i = 0; i < articles.length; i++) {
                                    articles[i].addEventListener('click', redirect, false);
                                }
                                function redirect() {
                                    for (var i = 0; i < articles.length; i++) {
                                        if (articles[i].classList.contains('active')) {
                                            articles[i].classList.remove('active');
                                        }
                                    }
                                    this.classList.add('active');
                                    document.getElementById("price_service").innerHTML = this.getAttribute('priceformat');
                                    serviceid.value = this.getAttribute('serviceid');
                                }
                            </script>
                            <div class="priceBlock">
                                <div class="priceBlockFixed">
                                    <div class="priceText priceTextHuyen typeRoom data_booking" id="price_service">89,000Đ</div>
                                </div>
                                <p class="text_2 wk-editable-text" id="sectionLP_11-text24">trên ngày</p>
                            </div>
